development:
  encoding: unicode
  adapter: postgresql
  host: <%= ENV['PACSCL_DB_HOST'] %>
  database: <%= ENV['PACSCL_DB_NAME'] %>
  username: <%= ENV['PACSCL_DB_USERNAME'] %>
  password: <%= ENV['PACSCL_DB_PASSWORD'] %>
  pool: 5
  min_messages: WARNING

